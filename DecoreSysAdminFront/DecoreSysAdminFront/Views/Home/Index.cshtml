<!-- Register Section -->
<section id="register" style="padding-top: 280px;">
    <div class="container">
        <div class="row">
            <form class="form-horizontal">
                <fieldset>
                    <!-- Firstname input-->
                    <div class="form-group">
                        <label class="col-md-12 control-label" for="firstnameinputRegister"></label>
                        <div class="col-md-12">
                            <h1 style="text-align: center; font-size: 50px;">Registrera ett nytt konto</h1>
                            <input id="firstnameinputRegister" name="firstnameinputRegister" type="text" placeholder="Förnamn" class="form-control input-md" required="">
                        </div>
                    </div>
                    <!-- Lastname input-->
                    <div class="form-group">
                        <label class="col-md-12 control-label" for="lastnameinputRegister"></label>
                        <div class="col-md-12">
                            <input id="lastnameinputRegister" name="lastnameinputRegister" type="text" placeholder="Efternamn" class="form-control input-md" required="">
                        </div>
                    </div>
                    <!-- Social Security Number input-->
                    <div class="form-group">
                        <label class="col-md-12 control-label" for="socsecInputRegister"></label>
                        <div class="col-md-12">
                            <input id="socsecInputRegister" name="socsecInputRegister" type="text" placeholder="Personnummer" class="form-control input-md" required="">
                        </div>
                    </div>
                    <!-- Email input-->
                    <div class="form-group">
                        <label class="col-md-12 control-label" for="emailInputRegister"></label>
                        <div class="col-md-12">
                            <input id="emailInputRegister" name="emailInputRegister" type="email" placeholder="Emailadress" class="form-control input-md" required="">
                        </div>
                    </div>
                    <!-- Password input-->
                    <div class="form-group">
                        <label class="col-md-12 control-label" for="passwordinputRegister"></label>
                        <div class="col-md-12">
                            <input id="passwordinputRegister" name="passwordinputRegister" type="password" placeholder="Lösenord" class="form-control input-md" required="">
                        </div>
                    </div>

                    <!-- Telephone number input-->
                    <div class="form-group">
                        <label class="col-md-12 control-label" for="telnumInputRegister"></label>
                        <div class="col-md-12">
                            <input id="telnumInputRegister" name="telnumInputRegister" type="tel" placeholder="Telefonnummer" class="form-control input-md" required="">
                        </div>
                    </div>
                    <!-- Submit Button -->
                    <div class="form-group" style="text-align: center;">
                        <label class="col-md-6 control-label" for="registerSubmit"></label>
                        <div class="col-md-12">
                            <button id="registerSubmit" name="registerSubmit" class="btn btn-success">Skapa konto</button>
                        </div>
                    </div>
                </fieldset>
            </form>
        </div>
    </div>
</section>

<!-- Login Section -->
<section id="login" style="padding-bottom: 330px;  padding-top: 350px;">
    <div class="container">
        <div class="row">
            <form class="form-horizontal" action="Home/Login" method="post">
                <fieldset>
                    <div class="form-group">
                        <label class="col-md-12 control-label" for="userinputLogin"></label>
                        <div class="col-md-12">
                            <h1 style="text-align: center; font-size: 50px;">Logga in på Decore</h1>
                            <input id="userinputLogin" name="userinputLogin" type="email" placeholder="Emailadress" class="form-control input-md" required="">
                        </div>
                    </div>
                    <!-- Password input-->
                    <div class="form-group">
                        <label class="col-md-12 control-label" for="passwordinput"></label>
                        <div class="col-md-12">
                            <input id="passwordinputLogin" name="passwordinputLogin" type="password" placeholder="Lösenord" class="form-control input-md" required="">
                        </div>
                    </div>
                    <!-- Submit button -->
                    <div class="form-group" style="text-align: center;">
                        <label class="col-md-6 control-label" for="loginSubmit"></label>
                        <div class="col-md-12">
                            <input type="submit" id="loginSubmit" name="loginSubmit" value="Logga in" class="btn btn-success" />
                        </div>
                    </div>
                </fieldset>
            </form>
        </div>
    </div>
</section>